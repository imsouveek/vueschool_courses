{
    "name": "vue-wordle",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    },
    "scripts": {
        "dev": "vite --host 0.0.0.0 --port 3009",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test": "vitest",
        "test:e2e": "playwright test",
        "test:e2e:report": "playwright show-report --host 0.0.0.0 --port 3010",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint": "eslint . --fix"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.12",
        "tailwindcss": "^4.1.12",
        "vue": "^3.5.18"
    },
    "devDependencies": {
        "@iconify/vue": "^5.0.0",
        "@playwright/test": "^1.55.0",
        "@tsconfig/node22": "^22.0.2",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.16.5",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.32.0",
        "eslint-config": "workspace:*",
        "jiti": "^2.5.1",
        "jsdom": "^26.1.0",
        "npm-run-all2": "^8.0.4",
        "tsconfig": "workspace:*",
        "typescript": "~5.8.0",
        "vite": "^7.0.6",
        "vite-plugin-checker": "^0.10.2",
        "vite-plugin-vue-devtools": "^8.0.0",
        "vitest": "^3.2.4",
        "vue-tsc": "^3.0.4"
    }
}
